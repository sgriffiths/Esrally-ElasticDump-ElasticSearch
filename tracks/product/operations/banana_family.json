                            "term": {
                              "price.fulfilmentstoreid": {
                                "value": "1649"
                              }
                            }
                          }
                        ]
                      }
                    },
                    "path": "price"
                  }
                },
                {
                  "terms": {
                    "shelfgroupid": [
                      "2835",
                      "738",
                      "400",
                      "1192",
                      "1193"
                    ]
                  }
                }
              ],
              "must_not": [
                {
                  "range": {
                    "agerestricted.raw": {
                      "gt": 2147483647.0,
                      "lt": 2147483647.0
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  }
}