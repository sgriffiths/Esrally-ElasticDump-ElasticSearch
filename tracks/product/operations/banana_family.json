    "size": 1000,
    "explain": false,
    "_source": {
      "includes": [
        "stockcode",
        "special",
        "shelfgroupkey",
        "shelfgroupid",
        "term"
      ],
      "excludes": [
        "price",
        "supplylimit",
        "priceexception",
        "suggest",
        "productgroupdisplayorder"
      ]
